<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size android:width="200dp" android:height="100dp"/>
            <stroke android:width="1dp" android:color="#156156"/>
            <solid android:color="#f9a3f5"/>
        </shape>
    </item>
    <item android:top= "2dp" android:bottom="2dp"
        android:right="2dp" android:left="2dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#894654"/>
        </shape>
    </item>
    <item android:top= "4dp" android:bottom="4dp"
        android:right="4dp" android:left="4dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#fa396f"/>
        </shape>
    </item>
</layer-list>